{
  "name": "Procrastinator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome addon for blocking distrating websties",
  "author": "Sebastian Kalinowski",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
